{
  "name": "sveltekit-fullstack-template",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --bun vite dev",
    "build": "bun --env-file=.env vite build",
    "db": "bun drizzle-kit",
    "prepare": "bun sync && bun lefthook install",
    "sync": "svelte-kit sync",
    "check": "bun types:check && bun lint:check && bun format:check",
    "fix": "bun lint:fix && bun format:fix",
    "types:check": "bun sync && svelte-check --tsconfig ./tsconfig.json",
    "format:check": "bun prettier . --check",
    "format:fix": " bun prettier . --write",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@iconify/svelte": "^5.0.1",
    "@libsql/client": "^0.15.10",
    "@sveltejs/adapter-auto": "^6.1.1",
    "@sveltejs/kit": "^2.27.1",
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@types/bun": "^1.2.19",
    "better-auth": "^1.3.4",
    "daisyui": "^5.0.50",
    "drizzle-orm": "^0.44.4",
    "drizzle-valibot": "^0.4.2",
    "runed": "0.31.1",
    "svelte": "^5.38.0",
    "svelte-check": "^4.3.1",
    "svelte-easy-crop": "^4.0.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2",
    "valibot": "^1.1.0",
    "vite": "^7.0.6"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.36.0",
    "@types/node": "^22",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.4",
    "globals": "^16.4.0",
    "lefthook": "^1.12.2",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript-eslint": "^8.44.1",
    "wrangler": "^4.28.0"
  }
}
